    $('[data-modal]:not(.modal)').click(function(){
        if ($(this).data('modal') == 'getCall') {
            $('#formHolder').prev('form').append($('#formHolder').html());
            $('#formHolder').remove();
        } else if ($(this).data('modal') == 'getJob') {
            $('input#job').val($(this).data('title'));
            $('#formHolderJob').prev('form').append($('#formHolderJob').html());
            $('#formHolderJob').remove();
        } else if ($(this).data('modal') == 'seminar') {
            $('input#event').val($('.pageTitle').text());
            var thisName = $('.pageTitle').text().charAt(0).toLowerCase() + $('.pageTitle').text().slice(1);
            $('#seminar .modalTitle').text('Запись на '+thisName);
            $('#formHolderSeminar').prev('form').append($('#formHolderSeminar').html());
            $('#formHolderSeminar').remove();
        } $('.modal[data-modal="'+$(this).data('modal')+'"]').addClass('open');
    });
    if ($('div').is('#formHolderWelcome')) {
        $('#formHolderWelcome').prev('form').html($('#formHolderWelcome').html());
