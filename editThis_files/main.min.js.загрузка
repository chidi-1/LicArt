    $('.catalogToggle, .catalogView').mouseenter(function(){
        var toToggle = $(this);
        toOut = setTimeout(function(){
            toToggle.closest('.menuWrapp').find('.catalogToggle').addClass('open');
            toToggle.closest('.menuWrapp').find('.catalogView').addClass('open');
        },300);
    }).mouseleave(function(){
        clearTimeout(toOut);
