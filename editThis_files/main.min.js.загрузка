            $('.events.byYear').html('');
            if ($(this).val() != '0') {
                $('#freezedEvents .event[data-event="'+$(this).val()+'"]').each(function(){
                    $(this).clone().appendTo('.events.byYear');
                }); paginationEventsReset();
            } else {
                $('.events.byYear').html('');
                $('#freezedEvents .event').each(function(){
                    $(this).clone().appendTo('.events.byYear');
                }); paginationEventsReset();
            }
        } else {
            if ($(this).val() != '0') {
                $('.event[data-event]').addClass('hidden');
                $('.event[data-event="'+$(this).val()+'"]').removeClass('hidden');
            } else {
                $('.event[data-event]').removeClass('hidden');
            }
        }
    });
