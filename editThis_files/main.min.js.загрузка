    $(window).scroll(function(){
        if ($(this).scrollTop() > 139) {
            $('.menuWrapp.menuFixed').addClass('isFixed');
        } else {
            $('.menuWrapp.menuFixed').removeClass('isFixed');
        }
        
        if ($(this).scrollTop() > 300) {
            $('.toTop').addClass('view');
        } else {
            $('.toTop').removeClass('view');
        }
        if ($(window).width() > 860) {
            $('.leftBar.toScroll').each(function(){
                var thisTop = this.getBoundingClientRect().top;
                var parentTop = this.closest('.pageTypeA').getBoundingClientRect().top;
                var parentHeigh = $(this).height();
                var childTop = this.lastElementChild.getBoundingClientRect().top;
                var childHeight = $(this).find('ul').height();
                if (thisTop < 90) {
                    $(this).find('ul').addClass('fixedBar');
                    // console.log(thisTop);
                    // console.log(childTop);
                    if ((parentTop+parentHeigh) - (childTop+childHeight) > 0 || childTop > 90) {
                        $(this).find('ul').css('top', -thisTop+90+'px');
                    }
                } else {
                    $(this).removeClass('fixedBar');
                }
            });
        }
        
        if ($('div').is('.contactItem')) {
            var allScroll = $(window).scrollTop() + $(window).height();
            var mapItemA = $('#ci0').offset().top;
            var mapItemB = $('#ci1').offset().top;
            var mapItemC = $('#ci2').offset().top;
            var mapItemD = $('#ci3').offset().top;
            var mapItemE = $('#ci4').offset().top;
            var mapItemF = $('#ci5').offset().top;
            var mapItemG = $('#ci6').offset().top;
            var mapItemH = $('#ci7').offset().top;
            // var mapItemI = $('#ci8').offset().top;
            // var mapItemJ = $('#ci9').offset().top;
            // var mapItemK = $('#ci10').offset().top;
            if (allScroll > mapItemA && !$('#ci0 iframe').hasClass('loaded')) {
                $('#ci0 iframe').addClass('loaded');
                $('#ci0 iframe').attr('src', $('#ci0 iframe').attr('data-src'));
            }
            if (allScroll > mapItemB && !$('#ci1 iframe').hasClass('loaded')) {
                $('#ci1 iframe').addClass('loaded');
                $('#ci1 iframe').attr('src', $('#ci1 iframe').attr('data-src'));
            }
            if (allScroll > mapItemC && !$('#ci2 iframe').hasClass('loaded')) {
                $('#ci2 iframe').addClass('loaded');
                $('#ci2 iframe').attr('src', $('#ci2 iframe').attr('data-src'));
            }
            if (allScroll > mapItemD && !$('#ci3 iframe').hasClass('loaded')) {
                $('#ci3 iframe').addClass('loaded');
                $('#ci3 iframe').attr('src', $('#ci3 iframe').attr('data-src'));
            }
            if (allScroll > mapItemE && !$('#ci4 iframe').hasClass('loaded')) {
                $('#ci4 iframe').addClass('loaded');
                $('#ci4 iframe').attr('src', $('#ci4 iframe').attr('data-src'));
            }
            if (allScroll > mapItemF && !$('#ci5 iframe').hasClass('loaded')) {
                $('#ci5 iframe').addClass('loaded');
                $('#ci5 iframe').attr('src', $('#ci5 iframe').attr('data-src'));
            }
            if (allScroll > mapItemG && !$('#ci6 iframe').hasClass('loaded')) {
                $('#ci6 iframe').addClass('loaded');
                $('#ci6 iframe').attr('src', $('#ci6 iframe').attr('data-src'));
            }
            if (allScroll > mapItemH && !$('#ci7 iframe').hasClass('loaded')) {
                $('#ci7 iframe').addClass('loaded');
                $('#ci7 iframe').attr('src', $('#ci7 iframe').attr('data-src'));
            }
            /* if (allScroll > mapItemI) {
                $('#ci8 iframe').attr('src', $('#ci8 iframe').attr('data-src'));
            }
            if (allScroll > mapItemJ) {
                $('#ci9 iframe').attr('src', $('#ci9 iframe').attr('data-src'));
            }
            if (allScroll > mapItemK) {
                $('#ci10 iframe').attr('src', $('#ci10 iframe').attr('data-src'));
            } */
        }
    });
