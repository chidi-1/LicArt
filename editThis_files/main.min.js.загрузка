	$('.labs-team-item-content-wrap .labs-team-item-content:nth-child(8)').addClass('labs-team-item-active');
});
$('.labs-team-item-content-wrap .labs-team-item-content:nth-child(9) .team-pointer').click(function(e) {
	$('.labs-team-item-content-wrap .labs-team-item-content').removeClass('labs-team-item-active');
	$('.labs-team-item-content-wrap .labs-team-item-content:nth-child(9)').addClass('labs-team-item-active');
});
