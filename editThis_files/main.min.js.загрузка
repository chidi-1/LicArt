$('.labs-team-show-more').click(function(e) {
	$('.labs-team-item').removeClass('notshowmob'); 
	$('.labs-team-show-more').css("display", "none");
});

$(function() { 
    var videos  = $(".tabs-content-v");

        videos.on("click", function(){
            var elm = $(this),
                conts   = elm.contents(),
                le      = conts.length,
                ifr     = null;

            for(var i = 0; i<le; i++){
              if(conts[i].nodeType == 8) ifr = conts[i].textContent;
            }

            elm.addClass("player").html(ifr);
            elm.off("click");
        });
});
if ($(window).width() < 900) {
   $('.labs-team-first-p').addClass('blockTitle');
}

$(document).on('focus','input[type="text"]',function(){
    console.log('ABT');
    var input = $(this)[0];
}).on('blur','input[type="text"]',function(){
    var input = $(this)[0];
    var repeated = input.value;
    repeated = repeated.replaceAll(' (', '');
    repeated = repeated.replaceAll(') ', '');
    repeated = repeated.replaceAll('-', '');
    if (repeated.indexOf('аааа') > -1 || repeated.indexOf('бббб') > -1 || repeated.indexOf('вввв') > -1 || repeated.indexOf('гггг') > -1 || repeated.indexOf('дддд') > -1 || repeated.indexOf('ееее') > -1 || repeated.indexOf('ёёёё') > -1 || repeated.indexOf('жжжж') > -1 || repeated.indexOf('зззз') > -1 || repeated.indexOf('ииии') > -1 || repeated.indexOf('йййй') > -1 || repeated.indexOf('кккк') > -1 || repeated.indexOf('лллл') > -1 || repeated.indexOf('мммм') > -1 || repeated.indexOf('нннн') > -1 || repeated.indexOf('оооо') > -1 || repeated.indexOf('пппп') > -1 || repeated.indexOf('рррр') > -1 || repeated.indexOf('сссс') > -1 || repeated.indexOf('тттт') > -1 || repeated.indexOf('уууу') > -1 || repeated.indexOf('фффф') > -1 || repeated.indexOf('хххх') > -1 || repeated.indexOf('цццц') > -1 || repeated.indexOf('чччч') > -1 || repeated.indexOf('шшшш') > -1 || repeated.indexOf('щщщщ') > -1 || repeated.indexOf('ъъъъ') > -1 || repeated.indexOf('ыыыы') > -1 || repeated.indexOf('ьььь') > -1 || repeated.indexOf('ээээ') > -1 || repeated.indexOf('юююю') > -1 || repeated.indexOf('яяяя') > -1) {
        input.value = '';
        console.log('AABB');
        //$(this).addClass('error');
    }
});