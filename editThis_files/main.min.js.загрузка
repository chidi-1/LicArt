        } $(this).parent().addClass('defaultGallery');
        $(this).replaceWith('<a href="'+$(this).attr('src')+'" data-fancybox="defaultGallery"><img src="'+$(this).attr('src')+'" style="'+$(this).attr('style')+'" width="'+$(this).attr('width')+'" height="'+$(this).attr('height')+'" /></a>'); 
